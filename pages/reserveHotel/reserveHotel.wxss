/* pages/reserveHotel/reserveHotel.wxss */
.slide-image{
  width: 100%;
}
.level{
  color: #52B5F0;
}
.score{
  background-color: #F9DCCE;
  color: #ED852D;
  border-radius: 3px;
  padding: 2px 8px;
  margin-left: 10px;
}
.good-s{
  margin-left: 10px;
}
.map-view,.config-view{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
 .map{
  width: 85px;
  background:  url(data:image/png;base64,) center right no-repeat;
  background-size: cover;
  text-align: center;
  line-height: 55px;
  color: #52B5F0;
} 
.map-sit,.config-sit{
  padding: 30rpx 10rpx 30rpx 30rpx;
  flex: 1;
}
.right-icon{
  width: 16rpx;
  margin-left: 10rpx;
  position: relative;
  top: 1.5px;
}
.config-d{
  width: 85px;
  text-align: center;
  line-height: 55px;
  color: #52B5F0;
}
.net,.parking,.traffic{
  width: 34rpx;
}
.config-sit{
  display: flex;
  flex-direction: row;
}
.parking,.traffic{
  position: relative;
  top: 5rpx;
}

.order-list{
  margin-top: 10px;
}
.checkInTime{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.date-time{
  border: 1px solid #52B5F0;
  width: 80px;
  text-align: center;
  font-size: 28rpx;
  border-radius: 20px;
  padding: 5rpx 0;
}
.data-v{
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.hoter-list{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding-bottom: 20rpx;
}
.coverImg{
  width: 180rpx;
}
.likeText{
  background-color: #EE7E31;
  color: #fff;
  font-size: 28rpx;
}
.h-desc{
  font-size: 30rpx;
}
.h-price{
  color: #EE7E31;
  margin-top: 10rpx;
}
.vip-price{
  color: #EE7E31;
  border: 1px solid #EE7E31;
  border-radius: 10px;
  padding: 0 10px;
  font-size: 28rpx;
  margin-left: 20rpx;
}
.h-container{
  display: flex;
  flex-direction: column;
  flex: 1;
  justify-content: center; 
  padding-left: 20rpx;
}
.or-btn{
  width: 100px;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.order-btn{
  background-color: #EE7E31;
  color: #fff;
  font-size: 28rpx;
  width: 60px;
  text-align: center;
  border: 1px solid #EE7E31;
  border-top-left-radius: 8rpx;
  border-top-right-radius: 8rpx;
}
.line-btn{
  color: #EE7E31;
  font-size: 28rpx;
  width: 60px;
  text-align: center;
  border: 1px solid #EE7E31;
  border-bottom-left-radius: 8rpx;
  border-bottom-right-radius: 8rpx;
}
.gDown > text{
  color: #52B5F0;
  font-size: 28rpx;
}
.gDown > image{
  width: 28rpx;
  margin-left: 5px;
}